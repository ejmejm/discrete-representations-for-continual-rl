{
  "name": "Explore Right Crossing | Continuous | FTA AE",

  "project": "discrete_mbrl-minigrid_trans_best",

  "command": "python full_train_eval.py",

  "algorithm": "grid",

  "spec": {
    "metric": "explore_right_state_distrib_kl_div",
