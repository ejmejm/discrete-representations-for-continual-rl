    "n_preload": 0,
    "n_train_unroll": 4,
    "rl_train_steps": 0,
    "trans_epochs": 35,
    "checkpoint_freq": 9,
    "epochs": 20,
    "ae_model_type": "fta_ae",
    "trans_model_type": "continuous",

    "env_name": "minigrid-crossing-stochastic",
    "eval_policies": "explore_right",
    "stochastic": "categorical",

    "trans_hidden": {
      "type": "discrete",
      "values": [16, 32, 64, 128, 256, 512, 1024],
      "dependents": {
        "latent_dim": {
          "type": "discrete",
          "values": [256, 64, 64, 64, 256, 64, 64]
        },
        "fta_tiles": {
          "type": "discrete",
          "values": [16, 16, 16, 8, 8, 16, 32]
        },
        "fta_eta": {
          "type": "discrete",
          "values": [0.25, 0.25, 0.25, 0.5, 0.5, 0.25, 0.125]
        }
      }
    }
  }
}