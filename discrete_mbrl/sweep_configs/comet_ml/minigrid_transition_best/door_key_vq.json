{
  "name": "Goal Door Key | Discrete | Hard VQVAE",

  "project": "discrete_mbrl-minigrid_trans_best",

  "command": "python full_train_eval.py",

  "algorithm": "grid",

  "spec": {
    "metric": "goal_state_distrib_kl_div",
    "objective": "minimize",
    "retryAssignLimit": 5
  },

  "trials": 25,

  "parameters": {
    "all_data": True,
    "no_load": True,
    "comet_ml": True,
    "exact_comp": True,
    "log_state_reprs": True,
    "log_freq": 500,
    "eval_unroll_steps": 30,
    "batch_size": 512,
    "embedding_dim": 64,
    "eval_batch_size": 128,
    "learning_rate": 0.0002,
    "n_preload": 0,
    "n_train_unroll": 4,
    "rl_train_steps": 0,
    "trans_epochs": 35,
    "checkpoint_freq": 9,
    "epochs": 20,
    "ae_model_type": "vqvae",
    "trans_model_type": "discrete",

    "env_name": "minigrid-door-key-stochastic",
    "eval_policies": "goal",
    "stochastic": "categorical",

    "trans_hidden": {
      "type": "discrete",
      "values": [16, 32, 64, 128, 256, 512, 1024],
      "dependents": {
        "filter_size": {
          "type": "discrete",
          "values": [3, 3, 6, 9, 3, 9, 3]
        },
        "codebook_size": {
          "type": "discrete",
          "values": [1024, 64, 1024, 16, 1024, 16, 256]
        }
      }
    }
  }
}