{
  "name": "Door Key | UCompact Embeds",

  "project": "discrete_mbrl-custom_embeds",

  "command": "python full_train_eval.py",

  "algorithm": "grid",

  "spec": {
    "metric": "goal_state_distrib_kl_div",
    "objective": "minimize",
    "retryAssignLimit": 3
  },

  "trials": 25,

  "parameters": {
    "all_data": True,
    "no_load": True,
    "exact_comp": True,
    "comet_ml": True,
    "log_freq": 2000,
    "eval_unroll_steps": 30,
    "batch_size": 512,
    "embedding_dim": 64,
    "eval_batch_size": 128,
    "learning_rate": 0.0002,
    "n_preload": 0,
    "n_train_unroll": 4,
    "rl_train_steps": 0,
    "trans_epochs": 40,
    "checkpoint_freq": 30,
    "epochs": 0,
    "trans_hidden": 64,
    "ae_model_type": "identity",
    "trans_model_type": "discrete",

    "env_name": "minigrid-door-key-stochastic-ucompact",
    "eval_policies": "goal",
    "stochastic": "categorical"
  }
}