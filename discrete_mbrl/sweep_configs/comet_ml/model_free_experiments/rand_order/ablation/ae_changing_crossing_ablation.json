{
  "name": "Changing Crossing | AE | Ablation",

  "project": "discrete_mbrl-mf_chainging_ablation",

  "command": "python model_free/train.py",

  "algorithm": "grid",

  "spec": {
    "metric": "reward",
    "objective": "maximize",
    "retryAssignLimit": 4
  },

  "trials": 5,

  "parameters": {
    "comet_ml": True,
    "ae_recon_loss": True,
    "ae_er_train": True,
    "device": "cuda",
    "log_freq": 2500,
    "learning_rate": 0.0003,
    "batch_size": 256,
    "mf_steps": 400000,
    "embedding_dim": 64,
    "env_change_freq": 40000,
    "env_max_steps": 400,
    "n_ae_updates": 8,
    "ppo_batch_size": 64,
    "ppo_value_coef": 0.5,

    "ae_model_type": "ae",
    "latent_dim": 1024,

    "env_name": "minigrid-crossing-stochastic",
    "stochastic": "categorical",

    "ppo_iters": {
      "type": "discrete",
      "values": [10, 20, 30, 40]
    },
    "ppo_clip": {
      "type": "discrete",
      "values": [0.1, 0.2, 0.3]
    }
  }
}